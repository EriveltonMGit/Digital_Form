{
  "version": 2,
  "builds": [
    {
      "src": "api/index.js",
      "use": "@vercel/node"
    }
  ],
  "routes": [
    { "src": "/clientes(.*)", "dest": "/api/server.mjs" },
    { "src": "/fornecedores(.*)", "dest": "/api/server_fornecedores.mjs" },
    { "src": "/produtos(.*)", "dest": "/api/server_products.mjs" }
  ]
}
